class Solution:
    def climbStairs(self, n: int) -> int:
        if n == 1:
            return 1
        if n == 2:
            return 2

        a, b = 1, 2  # f(1), f(2)
        for i in range(3, n + 1):
            a, b = b, a + b  # shift window forward
        return b

solution = Solution()

print(solution.climbStairs(5))

#time complexity is O(n)
