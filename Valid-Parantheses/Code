
class Solution:
    def isValid(self, s: str) -> bool:
        n = len(s)
        if n % 2 == 1:  
            return False
        
        for i in range(n // 2):  
            if s[i] == s[n - 1 - i] :
                return True  
            else:
                return False
solution = Solution()
print(solution.isValid("()[]{}")) 